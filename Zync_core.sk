command /spawn:
    trigger:
        teleport player to {spawn}
        send "{@PrefixCoulor}&lTeleported To Spawn!" to player
        play sound "minecraft:block.note_block.pling" at pitch 2 to player
        if player contains {alive::*}:
            remove {alive::*} from player
        if player does not contain {dead::*}:
            add {dead::*} to player
        if {recentrevive::*} does not contain player:
            add player to {recentrevive::*}
            wait 30 seconds
            remove player from {recentrevive::*}

command /revive [<player>]:
    permission: zynccore.admin
    permission message: &cYou Dont Have Permission To Use This Command!
    trigger:
        if arg-1 is a player:
            teleport arg-1 to player
            if player contains {dead::*}:
                remove {dead::*} from player
            if player does not contain {alive::*}:
                add {alive::*} to player
            
command /reviveall [<text>]:
    permission: zynccore.admin
    permission message: &cYou Dont Have Permission To Use This Command!
    trigger:
        if size of {dead::*} is bigger than 0:
            if arg-1 isnt set:
                loop players that contain {dead::*}:
                    teleport loop-player to player
                    if loop-player contains {dead::*}:
                        remove {dead::*} from loop-player
                    if loop-player does not contain {alive::*}:
                        add {alive::*} to loop-player
            f arg-1 is "fast":
                loop players that contain {dead::*}:
                    teleport loop-player to player
                    if loop-player contains {dead::*}:
                        remove {dead::*} from loop-player
                    if loop-player does not contain {alive::*}:
                        add {alive::*} to loop-player
            if arg-1 is "slow":
                loop players that contain {dead::*}:
                    teleport loop-player to player
                    if loop-player contains {dead::*}:
                        remove {dead::*} from loop-player
                    if loop-player does not contain {alive::*}:
                        add {alive::*} to loop-player
                    wait 1 tick
            
command /reviverecent:
    permission: zyncore.admin
    permission message: &cYou Dont Have Permission To Use This Command!
    trigger:
        if size of {reviverecent::*} is bigger than 0:
            add {reviverecent::*} to {alive::*}
            remove {recentrevive::*} from {dead::*}
            teleport players in {recentrevive::*} to sender
        loop all players:
            if {recentrevive::*} contains loop-player:
                remove loop-player from {recentrevive::*}
                add loop-player to {alive::*}
                remove loop-player from {dead::*}

