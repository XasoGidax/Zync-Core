            play sound "minecraft:block.note_block.pling" at pitch 2 to player




            





            #TabCompletes

on tab complete of "/reviveall", "/killall", "/killalive", "/killdead", "/tpdead", "/tpall", "/healall":
    if player has permission "zynccore.admin":
        set tab completions for position 1 to "slow" and "fast"
        
on tab complete of "/timer":
    if player has permission "zynccore.admin":
        set tab completions for position 2 to "seconds" and "minutes"

on tab complete of "/hide":
    if player has permission "zynccore.admin":
        set tab completions for position 1 to "all" and "host"

on tab complete of "/list":
    set tab completions for position 1 to "all" and "alive" and "dead"

on tab complete of "/core":
    set tab completions for position 1 to "help" and "version"

on tab complete of "/coreadmin":
    if player has permission "zynccore.admin":
        set tab completions for position 1 to "help" and "version" and "reload" and "set"
        if arg-2 is "set":
            set tab completions for position 2 to "title" and "Ip" and "HidePlugins" and "CustomJoinMessage" and "CustomLeaveMessage" and "CustomDeathMessage" and "NightVisonOnJoin" and "AnnounceTitle" and "PrefixCoulor" and "ScoreBoardCoulor" and "SkipsOnJoin" and "RevivesOnJoin" and "MuteNosises" and "PlayerJoinAlive" and "DisableOpCommand"

on tab complete of "/event":
    if player has permission "zynccore.admin":
        set tab completions for position 1 to "start" and "stop" and "set"
        