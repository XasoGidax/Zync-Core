command /timer [<integer>] [<text>]:
    permission: zynccore.admin
    permission message: &cYou Dont Have Permission To Use This Command!
    trigger:
        if arg-1 or arg-2 is not set:
            send "{@PrefixCoulor}&lPlease Use /timer <Amount Of Time> <Seconds/Minutes>" to player
        else:
            set {timer} to true
            if arg-1 is an integer:
                if {_time} isnt set:
                    if arg-2 is "seconds" or "second":
                        set {_time} to arg-1
                        while {_time} > 0:
                            send action bar "{@PrefixCoulor}&lTimer: &c%{_time}% &lSeconds" to all players
                            play sound "minecraft:block.note_block.pling" at volume 100 at pitch 2 to all players
                            wait 1 second
                            remove 1 from {_time}
                        broadcast "{@PrefixCoulor}&lTimer Has Ended!" to all players
                        play sound "minecraft:block.end_portal.spawn" at volume 100 at pitch 2 to all players
                        delete {_time}
                    if arg-2 is "minute" or "minutes":
                        set {_time} to arg-1
                        while {_time} > 0:
                            send action bar "{@PrefixCoulor}&lTimer: &c%{_time}% &lMinutes" to all players
                            play sound "minecraft:block.note_block.pling" at volume 100 at pitch 2 to all players
                            wait 1 minute
                            remove 1 from {_time}
                        broadcast "{@PrefixCoulor}&lTimer Has Ended!" to all players
                        play sound "minecraft:block.end_portal.spawn" at volume 100 at pitch 2 to all players
                    else:
                        send "{@PrefixCoulor}Stop: &cPlease Specify if you want the timer in seconds or minutes" to player
                        play sound "minecraft:block.note_block.bass" at volume 100 to player
                else:
                    send "{@PrefixCoulor}&lStop: &cPlease Specify A Number!" to player
                    play sound "minecraft:block.note_block.bass" at volume 100 to player
            else:
                send "{@PrefixCoulor}&lStop: &cYou Already Have A Timer Going Just &lWAIT" to player
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        else:
            send "{@PrefixCoulor}&lStop: &cPlease Specify A Number!" to player
            play sound "minecraft:block.note_block.bass" at volume 100 to player

